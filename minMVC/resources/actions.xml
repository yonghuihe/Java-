<?xml version="1.0" encoding="UTF-8"?>
<actions>
	<!-- 配置DepartmentAction -->
	<action name="department" class="com._520it.oa.web.action.DepartmentAction"
		method="excute"
		<!-- 
			result元素:表示结果视图
				name属性:逻辑视图名称
				type属性:跳转到物理视图的方式(dispatcher[请求转发]|redirect[utl重定向])
				文本内容    :物理视图			
		 -->
		<result name="list" type="dispatcher">/WEB-INF/views/employee/list.jsp</result>
		<result name="edit" type="redirect">/WEB-INF/views/employee/edit.jsp</result>
		/>
		<!-- 配置EmployeeAction -->
		<action name="employee" class="com._520it.oa.web.action.EmployeeAction"
			method="excute" />
</actions>